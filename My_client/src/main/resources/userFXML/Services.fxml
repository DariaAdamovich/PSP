<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="bodybg" stylesheets="@../css/background.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userFXML.Products">
    <children>
        <TableView fx:id="serviceTable" layoutX="19.0" layoutY="59.0" opacity="0.76" prefHeight="282.0" prefWidth="562.0">
            <columns>
                <TableColumn fx:id="name" minWidth="100.0" prefWidth="200.0" text="Название товара" />
                <TableColumn fx:id="country" prefWidth="180.0" text="Страна производителя" />
                <TableColumn fx:id="price" prefWidth="180.0" text="Цена" />
            </columns>
        </TableView>
        <Button fx:id="btnLogOut" layoutX="454.0" layoutY="14.0" mnemonicParsing="false" onAction="#onLogOut" styleClass="redline" stylesheets="@../css/stylesheet.css" text="Выйти из аккаунта" />
        <Button fx:id="btnBack" layoutX="510.0" layoutY="360.0" mnemonicParsing="false" onAction="#OnBack" prefHeight="25.0" prefWidth="69.0" styleClass="redline" stylesheets="@../css/stylesheet.css" text="Назад" />
      <Label layoutX="19.0" layoutY="14.0" text="Товары" textFill="#8875ea">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
