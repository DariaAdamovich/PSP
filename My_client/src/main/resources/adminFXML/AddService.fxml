<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="adminbg" stylesheets="@../css/background.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminFXML.AddUpdateService">
    <children>
        <TextField fx:id="nameField" layoutX="252.0" layoutY="110.0" />
        <TextField fx:id="countryField" layoutX="252.0" layoutY="151.0" />
        <TextField fx:id="priceField" layoutX="251.0" layoutY="192.0" />
      <Label layoutX="14.0" layoutY="14.0" text="Добавление товара" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
        <Button fx:id="btnLogOut" layoutX="451.0" layoutY="14.0" mnemonicParsing="false" onAction="#onLogOut" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Выйти из аккаунта" />
        <Button fx:id="btnBack" layoutX="520.0" layoutY="353.0" mnemonicParsing="false" onAction="#onBack" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Назад" />
        <Label layoutX="79.0" layoutY="154.0" text="Страна производителя: " textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="13.0" />
         </font></Label>
        <Label layoutX="201.0" layoutY="195.0" text="Цена: " textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="13.0" />
         </font></Label>
        <Button fx:id="btnSave" layoutX="289.0" layoutY="240.0" mnemonicParsing="false" onAction="#onSave" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Сохранить" />
      <Label layoutX="127.0" layoutY="113.0" text="Название товар: " textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="13.0" />
         </font></Label>
    </children>
</AnchorPane>
